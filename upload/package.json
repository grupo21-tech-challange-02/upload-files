{
  "name": "upload",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:env && ng serve",
    "build": "npm run build:env && ng build",
    "build:env": "node scripts/generate-env.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:single-spa:upload": "ng build upload --configuration production",
    "serve:single-spa:upload": "ng serve --host 0.0.0.0 --port 8081 --disable-host-check"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.4.2",
  "dependencies": {
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/router": "^18.0.0",
    "rxjs": "~7.8.0",
    "single-spa": ">=4.0.0",
    "single-spa-angular": "9.2.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^18.0.0",
    "@angular/build": "^18.0.0",
    "@angular/cli": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "dotenv": "^17.2.3",
    "jsdom": "^27.1.0",
    "less": "^4.2.0",
    "single-spa-angular": "^9.2.0",
    "style-loader": "^3.3.1",
    "typescript": "~5.5.4",
    "vitest": "^4.0.8"
  }
}